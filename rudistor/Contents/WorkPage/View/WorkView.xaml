                    <Button Content="重置仓位" Grid.Column="2" Grid.Row="2" Style="{StaticResource ButtonStyle}" Command="{Binding WorkerViewModel.ResetPosition, Mode=OneWay, Source={StaticResource Locator}}" CommandParameter="GridE"/>
                    <Label Content="策略" Grid.Column="2" Grid.Row="3"/>
                    <ComboBox   SelectedValue="{Binding t2cl}" Grid.Column="3" Grid.Row="0"  Style="{StaticResource ComboStyle}" ItemsSource="{Binding WorkerViewModel.T2ComboboxItems, Source={StaticResource Locator}}" DisplayMemberPath="describe" SelectedValuePath="index" />
                    <xctk:IntegerUpDown Style="{StaticResource Integerupdown}"  FormatString="N0" Value="{Binding t2vol}" Increment="1"  Maximum="2000" Minimum="0" Grid.Column="3" Grid.Row="1"/>

                    <ComboBox   Grid.Column="3" Grid.Row="3"  SelectedValue="{Binding cl}" Style="{StaticResource ComboStyle}" ItemsSource="{Binding WorkerViewModel.ClComboboxItems, Source={StaticResource Locator}}" DisplayMemberPath="describe" SelectedValuePath="index"/>
                </Grid>
            </Grid>

        </Border>
        <Border BorderThickness="2" CornerRadius="5" Grid.Column="2" Grid.Row="2" x:Name="GridF" DataContext="{Binding StrategyF}">
            <Grid Grid.Column="0"  HorizontalAlignment="Stretch"  Margin="0" Grid.Row="1" VerticalAlignment="Stretch"  Background="#FFDAD6D6">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0" x:Name="GridF_up" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"  Background="#FFDAD6D6">
                    <Grid.RowDefinitions>
                        <RowDefinition x:Name="GridF_up_rowA" MinHeight="40"/>
                        <RowDefinition x:Name="GridF_up_rowB"/>
                        <RowDefinition x:Name="GridF_up_rowC"/>
                        <RowDefinition x:Name="GridF_up_rowD"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition x:Name="GridF_up_colA"/>
                        <ColumnDefinition x:Name="GridF_up_colB"/>
                        <ColumnDefinition x:Name="GridF_up_colC"/>
                        <ColumnDefinition x:Name="GridF_up_colD"/>
                        <ColumnDefinition x:Name="GridF_up_colE"/>
                        <ColumnDefinition x:Name="GridF_up_colF"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="{Binding StageId}" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" VerticalAlignment="Top"></Label>
                    <Label Content="总仓位限制 4" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" VerticalAlignment="Bottom"></Label>
                    <Label Content="{Binding limit, Mode=OneWay,Converter={StaticResource LabelConverter},ConverterParameter=限仓}" Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="1" VerticalAlignment="Stretch"></Label>
                    <Label Content="{Binding lockNum, Mode=OneWay,Converter={StaticResource LabelConverter},ConverterParameter=锁定}" Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="1" VerticalAlignment="Stretch"></Label>
                    <Label Content="{Binding vol, Mode=OneWay,Converter={StaticResource LabelConverter},ConverterParameter=每笔}" Grid.Column="0" Grid.Row="3" Grid.ColumnSpan="1" VerticalAlignment="Stretch"></Label>

                    <xctk:IntegerUpDown Style="{StaticResource Integerupdown}"   FormatString="N0" Value="{Binding limit}" Increment="1" Maximum="1000" Minimum="0" Grid.Column="1" Grid.Row="1"/>
                    <xctk:IntegerUpDown Style="{StaticResource Integerupdown}"  FormatString="N0" Value="{Binding lockNum}" Increment="1" Maximum="1000" Minimum="0" Grid.Column="1" Grid.Row="2"/>
                    <xctk:IntegerUpDown Style="{StaticResource Integerupdown}"   FormatString="N0" Value="{Binding vol}" Increment="1" Maximum="1000" Minimum="0" Grid.Column="1" Grid.Row="3"/>


                    <Button Content="修改" Grid.Column="2" Grid.Row="0" Style="{StaticResource ButtonStyle}" Command="{Binding WorkerViewModel.ModifyStage, Mode=OneWay, Source={StaticResource Locator}}" CommandParameter="GridF"/>
                    <Label Content="空开价差：" Grid.Column="2" Grid.Row="2" Grid.ColumnSpan="1" VerticalAlignment="Top"></Label>
                    <Label Content="{Binding kkjc, Mode=OneWay}" Grid.Column="2" Grid.Row="2" Grid.ColumnSpan="1" VerticalAlignment="Bottom"></Label>
                    <Label Content="多开价差：" Grid.Column="2" Grid.Row="3" Grid.ColumnSpan="1" VerticalAlignment="Top"></Label>
                    <Label Content="{Binding dkjc, Mode=OneWay}" Grid.Column="2" Grid.Row="3" Grid.ColumnSpan="1" VerticalAlignment="Bottom"></Label>
                    <Label Content="OFF" Visibility="{Binding IsNotActivate, Converter={StaticResource BoolVisibilityConverter}}" Style="{StaticResource LabelStyle}" Grid.Column="3" Grid.Row="0" Grid.ColumnSpan="1" VerticalAlignment="Stretch" Background="Red"></Label>
                    <Label Content="ON"  Visibility="{Binding IsActivate, Converter={StaticResource BoolVisibilityConverter}}" Style="{StaticResource LabelStyle}" Grid.Column="3" Grid.Row="0" Grid.ColumnSpan="1" VerticalAlignment="Stretch" Background="Green"></Label>

                    <xctk:DecimalUpDown   Style="{StaticResource Decimalupdown}"  FormatString="F2" Value="{Binding kkjc}" Increment="{Binding WorkerViewModel.GridFIncre,Source={StaticResource Locator}}"  Maximum="10000" Minimum="-10000" Grid.Column="3" Grid.Row="2"/>
                    <xctk:DecimalUpDown   Style="{StaticResource Decimalupdown}"  FormatString="F2" Value="{Binding dkjc}" Increment="{Binding WorkerViewModel.GridFIncre,Source={StaticResource Locator}}"  Maximum="10000" Minimum="-10000" Grid.Column="3" Grid.Row="3"/>


                    <Button Content="开启策略" Visibility="{Binding IsNotActivate, Converter={StaticResource BoolVisibilityConverter}}" Grid.Column="4" Grid.Row="0" Style="{StaticResource ButtonStyle}" Command="{Binding WorkerViewModel.Activate, Mode=OneWay, Source={StaticResource Locator}}" CommandParameter="GridF"/>
                    <Button Content="关闭策略" Visibility="{Binding IsActivate, Converter={StaticResource BoolVisibilityConverter}}" Grid.Column="4" Grid.Row="0" Style="{StaticResource ButtonStyle}" Command="{Binding WorkerViewModel.Activate, Mode=OneWay, Source={StaticResource Locator}}" CommandParameter="GridF"/>
                    <Label Content="空平：" Grid.Column="4" Grid.Row="2" Grid.ColumnSpan="1" VerticalAlignment="Top"></Label>
                    <Label Content="{Binding kp, Mode=OneWay}" Grid.Column="4" Grid.Row="2" Grid.ColumnSpan="1" VerticalAlignment="Bottom"></Label>
                    <Label Content="多平：" Grid.Column="4" Grid.Row="3" Grid.ColumnSpan="1" VerticalAlignment="Top"></Label>
                    <Label Content="{Binding dp, Mode=OneWay}" Grid.Column="4" Grid.Row="3" Grid.ColumnSpan="1" VerticalAlignment="Bottom"></Label>
                    <Button Content="发送参数" Grid.Column="5" Grid.Row="0"  Style="{StaticResource ButtonStyle}" Command="{Binding WorkerViewModel.Send, Mode=OneWay, Source={StaticResource Locator}}" CommandParameter="GridF"/>
                    <xctk:DecimalUpDown  Style="{StaticResource Decimalupdown}"  FormatString="F2" Value="{Binding kp}" Increment="{Binding WorkerViewModel.GridFIncre,Source={StaticResource Locator}}"  Maximum="10000" Minimum="-10000" Grid.Column="5" Grid.Row="2"/>
                    <xctk:DecimalUpDown   Style="{StaticResource Decimalupdown}"  FormatString="F2" Value="{Binding dp}" Increment="{Binding WorkerViewModel.GridFIncre,Source={StaticResource Locator}}"  Maximum="10000" Minimum="-10000" Grid.Column="5" Grid.Row="3"/>

                </Grid>
                <Grid Grid.Row="1" x:Name="GridF_down" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                    <Grid.RowDefinitions>
                        <RowDefinition x:Name="GridF_down_rowA"/>
                        <RowDefinition x:Name="GridF_down_rowB"/>
                        <RowDefinition x:Name="GridF_down_rowC"/>
                        <RowDefinition x:Name="GridF_down_rowD"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition x:Name="GridF_down_colA"/>
                        <ColumnDefinition x:Name="GridF_down_colB"/>
                        <ColumnDefinition x:Name="GridF_down_colC"/>
                        <ColumnDefinition x:Name="GridF_down_colD"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="{Binding t1cj, Mode=OneWay,Converter={StaticResource LabelConverter},ConverterParameter=腿1超价}" Grid.Column="0" Grid.Row="0" Background="{x:Null}"></Label>
                    <Label Content="{Binding t1dd, Mode=OneWay,Converter={StaticResource LabelConverter},ConverterParameter=腿1等待}" Grid.Column="0" Grid.Row="1" Background="{x:Null}"></Label>
                    <Label Content="{Binding t2cj, Mode=OneWay,Converter={StaticResource LabelConverter},ConverterParameter=腿2超价}" Grid.Column="0" Grid.Row="2" Background="{x:Null}"></Label>
                    <Label Content="{Binding t2dd, Mode=OneWay,Converter={StaticResource LabelConverter},ConverterParameter=腿2等待}" Grid.Column="0" Grid.Row="3" Background="{x:Null}"></Label>
                    <xctk:IntegerUpDown Style="{StaticResource Integerupdown}"   FormatString="N0" Value="{Binding t1cj}" Increment="1"  Maximum="100" Minimum="-100" Grid.Column="1" Grid.Row="0"/>
                    <xctk:IntegerUpDown Style="{StaticResource Integerupdown}"   FormatString="N0" Value="{Binding t1dd}" Increment="1"  Maximum="10" Minimum="0" Grid.Column="1" Grid.Row="1"/>
                    <xctk:IntegerUpDown Style="{StaticResource Integerupdown}"   FormatString="N0" Value="{Binding t2cj}" Increment="1"  Maximum="100" Minimum="-100" Grid.Column="1" Grid.Row="2"/>
                    <xctk:IntegerUpDown Style="{StaticResource Integerupdown}"   FormatString="N0" Value="{Binding t2dd}" Increment="1"  Maximum="10" Minimum="0" Grid.Column="1" Grid.Row="3"/>

                    <Label Content="腿2策略" Grid.Column="2" Grid.Row="0"/>
                    <Label Content="{Binding t2vol, Mode=OneWay,Converter={StaticResource LabelConverter},ConverterParameter=PermitVol}" Grid.Column="2" Grid.Row="1"/>
                    <Button Content="重置仓位" Grid.Column="2" Grid.Row="2" Style="{StaticResource ButtonStyle}" Command="{Binding WorkerViewModel.ResetPosition, Mode=OneWay, Source={StaticResource Locator}}" CommandParameter="GridF"/>
                    <Label Content="策略" Grid.Column="2" Grid.Row="3"/>
                    <ComboBox   SelectedValue="{Binding t2cl}" Grid.Column="3" Grid.Row="0"  Style="{StaticResource ComboStyle}" ItemsSource="{Binding WorkerViewModel.T2ComboboxItems, Source={StaticResource Locator}}" DisplayMemberPath="describe" SelectedValuePath="index" />
                    <xctk:IntegerUpDown Style="{StaticResource Integerupdown}"  FormatString="N0" Value="{Binding t2vol}" Increment="1"  Maximum="2000" Minimum="0" Grid.Column="3" Grid.Row="1"/>

                    <ComboBox   Grid.Column="3" Grid.Row="3"  SelectedValue="{Binding cl}" Style="{StaticResource ComboStyle}" ItemsSource="{Binding WorkerViewModel.ClComboboxItems, Source={StaticResource Locator}}" DisplayMemberPath="describe" SelectedValuePath="index"/>
                </Grid>
            </Grid>

        </Border>


    </Grid>
</UserControl>